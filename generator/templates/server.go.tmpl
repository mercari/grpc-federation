	return slog.GroupValue(
		{{- range .Attrs }}
		slog.{{ .Type }}("{{ .Key }}", {{ .Value }}),
		{{- end }}
	)
}
{{- else }}
func (s *{{ $serviceName }}) {{ .Name }}(v {{ .ValueType }}) slog.Value {
